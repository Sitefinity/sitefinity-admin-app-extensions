{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "extensions-cli": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "",
                        "main": "src/__extensions_index.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "extractLicenses": false,
                        "customWebpackConfig": {
                            "path": "./build/webpack-custom.ts",
                            "mergeStrategies": {
                                "externals": "replace"
                            }
                        },
                        "vendorChunk": false
                    },
                    "configurations": {
                        "prod": {
                            "customWebpackConfig": {
                                "path": "./build/webpack-custom.prod.ts",
                                "mergeStrategies": {
                                    "externals": "replace"
                                }
                            }
                        },
                        "dev": {
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "./node_modules/@progress/sitefinity-adminapp-sdk/wwwroot",
                                    "output": "./"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "browserTarget": "extensions-cli:build:dev"
                    }
                }
            }
        }
    },
    "defaultProject": "extensions-cli"
}
